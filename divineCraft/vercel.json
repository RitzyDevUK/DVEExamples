{
    "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
    "rewrites": [
        { "source": "/assets/(.*)", "destination": "/assets/$1" }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" }
            ]
        }
    ]
}
